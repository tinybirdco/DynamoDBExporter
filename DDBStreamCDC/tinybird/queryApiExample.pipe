    %
    SELECT 
      JSONExtractString(Keys, 'id') as id, 
      JSONExtractUInt(NewImage, 'age') as user_age,
      toDateTime(ApproximateCreationDateTime) as ApproximateCreationDateTime,
      eventSource, eventName
